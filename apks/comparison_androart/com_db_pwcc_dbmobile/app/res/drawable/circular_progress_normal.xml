<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="ring" android:innerRadiusRatio="2.5" android:useLevel="false" android:thickness="@dimen/progress_chart_border_thickness">
            <solid android:color="@color/colorProgressCircleBackground" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <shape android:shape="ring" android:innerRadiusRatio="2.5" android:useLevel="true" android:thickness="@dimen/progress_chart_border_thickness">
            <solid android:color="@color/colorProgressCircleProgressNormal" />
        </shape>
    </item>
</layer-list>