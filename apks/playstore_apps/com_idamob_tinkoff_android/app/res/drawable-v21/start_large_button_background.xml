<?xml version="1.0" encoding="utf-8"?>
<ripple n1:color="@color/start_large_button_background_ripple"
  xmlns:n1="http://schemas.android.com/apk/res/android">
    <item n1:id="@android:id/mask">
        <shape n1:shape="rectangle">
            <solid n1:color="@color/white" />
            <corners n1:radius="@dimen/button_corner_double" />
        </shape>
    </item>
    <item>
        <shape n1:shape="rectangle">
            <solid n1:color="@color/start_large_button_background_normal" />
            <corners n1:radius="@dimen/button_corner_double" />
            <stroke n1:width="0.5dip" n1:color="@color/transparent" />
        </shape>
    </item>
</ripple>