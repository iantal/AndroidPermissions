<?xml version="1.0" encoding="utf-8"?>
<ripple n1:color="@color/white_half_transparent"
  xmlns:n1="http://schemas.android.com/apk/res/android">
    <item n1:id="@android:id/mask">
        <shape>
            <solid n1:color="@color/white" />
            <stroke n1:width="0.5dip" n1:color="@color/white_half_transparent" />
            <corners n1:radius="@dimen/button_corner_double" />
        </shape>
    </item>
    <item>
        <shape>
            <solid n1:color="@color/transparent" />
            <stroke n1:width="0.5dip" n1:color="@color/white_half_transparent" />
            <corners n1:radius="@dimen/button_corner_double" />
        </shape>
    </item>
</ripple>