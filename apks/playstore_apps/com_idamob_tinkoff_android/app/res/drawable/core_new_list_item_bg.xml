<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item n1:state_selected="false"
      xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape n1:shape="rectangle">
            <corners n1:radius="@dimen/core_new_list_item_radius" />
            <solid n1:color="@color/core_list_item_not_selected" />
        </shape>
    </item>
    <item n2:state_selected="true"
      xmlns:n2="http://schemas.android.com/apk/res/android">
        <shape n2:shape="rectangle">
            <corners n2:radius="@dimen/core_new_list_item_radius" />
            <solid n2:color="@color/core_list_item_selected" />
        </shape>
    </item>
</selector>