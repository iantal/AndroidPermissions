<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item n1:state_selected="false" n1:state_pressed="false"
      xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape n1:shape="oval">
            <solid n1:color="@color/core_boolean_button_normal" />
        </shape>
    </item>
    <item n2:state_selected="false" n2:state_pressed="true"
      xmlns:n2="http://schemas.android.com/apk/res/android">
        <shape n2:shape="oval">
            <solid n2:color="@color/core_boolean_button_pressed" />
        </shape>
    </item>
    <item n3:state_selected="true" n3:state_pressed="false"
      xmlns:n3="http://schemas.android.com/apk/res/android">
        <shape n3:shape="oval">
            <solid n3:color="@color/core_boolean_button_selected" />
        </shape>
    </item>
    <item n4:state_selected="true" n4:state_pressed="true"
      xmlns:n4="http://schemas.android.com/apk/res/android">
        <shape n4:shape="oval">
            <solid n4:color="@color/core_boolean_button_selected_pressed" />
        </shape>
    </item>
</selector>