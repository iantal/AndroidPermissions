<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item n1:state_enabled="false"
      xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape n1:shape="rectangle">
            <corners n1:radius="@dimen/core_button_radius" />
            <solid n1:color="@color/core_transparent" />
            <stroke n1:width="@dimen/core_previous_btn_outline_width" n1:color="@color/core_previous_btn_outline_disabled" />
        </shape>
    </item>
    <item n2:state_enabled="true" n2:state_pressed="true"
      xmlns:n2="http://schemas.android.com/apk/res/android">
        <shape n2:shape="rectangle">
            <corners n2:radius="@dimen/core_button_radius" />
            <solid n2:color="@color/core_transparent" />
            <stroke n2:width="@dimen/core_previous_btn_outline_width" n2:color="@color/core_previous_btn_outline_disabled" />
        </shape>
    </item>
    <item n3:state_enabled="true" n3:state_pressed="false"
      xmlns:n3="http://schemas.android.com/apk/res/android">
        <shape n3:shape="rectangle">
            <corners n3:radius="@dimen/core_button_radius" />
            <solid n3:color="@color/core_transparent" />
            <stroke n3:width="@dimen/core_previous_btn_outline_width" n3:color="@color/core_previous_btn_outline_enabled" />
        </shape>
    </item>
</selector>