<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item n1:state_enabled="false"
      xmlns:n1="http://schemas.android.com/apk/res/android">
        <bitmap n1:src="@drawable/core_arrow_up" n1:alpha="@dimen/core_previous_button_pressed_alpha" />
    </item>
    <item n2:state_enabled="true" n2:state_pressed="true"
      xmlns:n2="http://schemas.android.com/apk/res/android">
        <bitmap n2:src="@drawable/core_arrow_up" n2:alpha="@dimen/core_previous_button_pressed_alpha" />
    </item>
    <item n3:state_enabled="true" n3:state_pressed="false"
      xmlns:n3="http://schemas.android.com/apk/res/android">
        <bitmap n3:src="@drawable/core_arrow_up" />
    </item>
</selector>