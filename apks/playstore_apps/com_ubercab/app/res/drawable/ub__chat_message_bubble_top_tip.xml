    <item android:height="@dimen/ui__spacing_unit_2x" android:width="@dimen/ui__spacing_unit_2x" android:left="@dimen/ui__spacing_unit_3x">
        <rotate android:fromDegrees="45.0" android:toDegrees="45.0" android:pivotX="0.0%" android:pivotY="0.0%">
            <shape android:shape="rectangle">
                <solid android:color="@color/ub__ui_core_brand_grey_40" />
