<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="?dividerWidth" android:color="@color/ub__pool_configuration_container_border_color" />
            <solid android:color="@color/ub__pool_configuration_toggle_solid_color" />
            <corners android:topLeftRadius="@dimen/ui__corner_radius" android:bottomLeftRadius="@dimen/ui__corner_radius" />
        </shape>
    </item>
    <item android:left="@dimen/ui__corner_radius" android:top="?dividerWidth" android:bottom="?dividerWidth">
        <shape android:shape="rectangle">
            <solid android:color="@color/ub__pool_configuration_toggle_solid_color" />
        </shape>
    </item>
</layer-list>