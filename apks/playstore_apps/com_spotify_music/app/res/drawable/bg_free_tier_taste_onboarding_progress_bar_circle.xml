<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/secondaryProgress">
        <shape android:shape="ring" android:innerRadiusRatio="2.6" android:thicknessRatio="9.0" android:useLevel="true">
            <gradient android:startColor="@color/glue_gray_30" android:endColor="@color/glue_gray_30" android:type="sweep" android:centerColor="@color/glue_gray_30" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <rotate android:fromDegrees="270.0" android:toDegrees="270.0" android:pivotX="50.0%" android:pivotY="50.0%">
            <shape android:shape="ring" android:innerRadiusRatio="2.6" android:thicknessRatio="9.0" android:useLevel="true">
                <gradient android:startColor="@color/glue_green" android:endColor="@color/glue_green" android:type="sweep" android:centerColor="@color/glue_green" />
            </shape>
        </rotate>
    </item>
</layer-list>