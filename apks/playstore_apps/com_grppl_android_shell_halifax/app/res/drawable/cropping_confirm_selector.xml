<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item n1:state_pressed="true"
      xmlns:n1="http://schemas.android.com/apk/res/android">
        <bitmap n1:gravity="center" n1:src="@drawable/ic_action_done" n1:tint="@color/croppingButtonPressTint" />
    </item>
    <item n2:state_enabled="true"
      xmlns:n2="http://schemas.android.com/apk/res/android">
        <bitmap n2:gravity="center" n2:src="@drawable/ic_action_done" />
    </item>
    <item n3:state_enabled="false"
      xmlns:n3="http://schemas.android.com/apk/res/android">
        <bitmap n3:gravity="center" n3:src="@drawable/ic_action_done" n3:tint="@color/croppingButtonDisabledTint" />
    </item>
</selector>