                <TextView android:textColor="@color/text_title" android:id="@id/tv_empty_list" android:visibility="gone" android:text="@string/err_no_branch_atm_available" android:layout_centerInParent="true" style="@style/TextBoldAllWrap" />
                <ListView android:id="@id/list_view_atm_branch" android:fadingEdge="none" android:cacheColorHint="@color/transparent" android:divider="@color/separator_bg" android:dividerHeight="1.0dip" style="@style/ListTheme" />
